# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# API Configuration
EXPO_PUBLIC_API_BASE_URL=http://localhost:8000
EXPO_PUBLIC_MCP_SERVER_URL=http://localhost:8001

# App Configuration
EXPO_PUBLIC_APP_NAME=Omnara
EXPO_PUBLIC_APP_SCHEME=omnara

# Google Sign-In Configuration
# Get your Web Client ID from Google Cloud Console
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_web_client_id
# iOS Client ID is required for iOS builds
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id
# Android Client ID is required for Android builds
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your_google_android_client_id

EXPO_PUBLIC_REVENUECAT_API_KEY=your_revenuecat_key

# Sentry
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn
# SENTRY_AUTH_TOKEN is required for sourcemap uploads; set it in your build environment (EAS/CI secret) instead of .env files.
