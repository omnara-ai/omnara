{
  "name": "Omnara GitHub Integration",
  "description": "AI-powered code assistant that helps with pull requests and issues using Omnara headless",
  "url": "https://omnara.com",
  "hook_attributes": {
    "url": "https://your-webhook-server.com/github/webhooks",
    "active": true
  },
  "public": false,
  "default_events": [
    "issues",
    "issue_comment",
    "pull_request",
    "pull_request_review",
    "pull_request_review_comment"
  ],
  "default_permissions": {
    "actions": "write",
    "contents": "write",
    "issues": "write",
    "metadata": "read",
    "pull_requests": "write"
  }
}