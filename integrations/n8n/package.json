{
  "name": "n8n-nodes-omnara",
  "version": "0.1.4",
  "description": "n8n node for Omnara - communicate with AI agents",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node-package",
    "omnara",
    "ai",
    "agents",
    "workflow"
  ],
  "license": "MIT",
  "homepage": "https://github.com/omnara-ai/omnara/tree/main/integrations/n8n",
  "author": {
    "name": "Omnara",
    "email": "contact@omnara.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omnara-ai/omnara.git",
    "directory": "integrations/n8n"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp src/nodes/Omnara/omnara.png dist/nodes/Omnara/ && cp src/nodes/Omnara/Omnara.node.json dist/nodes/Omnara/",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OmnaraApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Omnara/Omnara.node.js",
      "dist/nodes/OmnaraWebhook/OmnaraWebhook.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=20.15.0"
  }
}
